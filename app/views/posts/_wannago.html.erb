<%= post.title %>
<% if post.wannago_listed_by?(current_user) %>
	<%= link_to post_wannago_lists_path(post), method: :delete do %> 
		<i class="fas fa-calendar-check"><%= post.wannago_lists.count %></i>
	<% end %>
<% else %>
	<%= link_to post_wannago_lists_path(post), method: :post do %> 
		<i class="far fa-calendar-check"><%= post.wannago_lists.count %></i>
	<% end %>
<% end %>